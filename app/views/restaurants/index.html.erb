<div class="container">
  <h2>All Restuarants</h2>

  <ul class="list-group">
    <% @restaurants.each do |restaurant| %>
      <li class="list-group-item list-group-item-action"><%= link_to restaurant.name, restaurant %></li>
    <% end %>
  </ul>

  <%= link_to 'Add restaurant', new_restaurant_path %>
</div>
